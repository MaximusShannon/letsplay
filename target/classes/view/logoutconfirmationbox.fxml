<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="confirmBoxStage" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #292E37;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LogoutConfirmationBoxController">
   <children>
      <Text fill="WHITE" layoutX="109.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Leaving so soon?">
         <font>
            <Font name="Segoe UI" size="24.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="97.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Are you sure you want to logout?">
         <font>
            <Font name="Segoe UI" size="14.0" />
         </font>
      </Text>
      <Button fx:id="logoutProceed" layoutX="36.0" layoutY="138.0" mnemonicParsing="false" onAction="#logout" prefHeight="37.0" prefWidth="122.0" stylesheets="@../css/Sharp.css" text="Proceed" textFill="WHITE" />
      <Button fx:id="logoutCancel" layoutX="242.0" layoutY="138.0" mnemonicParsing="false" onAction="#cancelLogout" prefHeight="37.0" prefWidth="122.0" style="-fx-background-color: red;" stylesheets="@../css/Sharp.css" text="Cancel" textFill="WHITE" />
   </children>
</AnchorPane>
